    "name": "cgdsoftware/tasks",
    "description": "Task management dependency for Laravel Enso",
    "keywords": [
        "cgdsoftware",
        "tasks",
        "task-management"
    ],
    "homepage": "https://github.com/cgdsoftware/tasks",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Ocneanu",
