    "require": {
        "cgdsoftware/core": "^8.0",
        "cgdsoftware/dynamic-methods": "^2.0",
        "cgdsoftware/forms": "^4.0",
        "cgdsoftware/helpers": "^2.0",
        "cgdsoftware/tables": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelEnso\\Tasks\\": "src/",
            "LaravelEnso\\Tasks\\Database\\Factories\\": "database/factories/"
